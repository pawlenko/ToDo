<div class="col-xs-6 col-sm-4 col-lg-3" id="team_<%= team.id %>" >
<div class="thumbnail" >
    <div class="caption">
        <h3><%= team.name %></h2>
        <p class="flex-text text-muted"> <%= team.description %>  </p>
        <a class="btn btn-primary" href="#">Open</a>
        <%= link_to  "Remove", team_url(team),method: :delete,:class=>"btn btn-danger",:style=>"margin-top:10px;",:remote=>true %>
    </div>
</div>
</div>
